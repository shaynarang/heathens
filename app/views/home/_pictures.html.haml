#pictures_content
  - @galleries.each do |gallery|
    - gallery.images.each do |image|
      .col-md-4.col-sm-4.col-xs-12
        %a.fancybox{:href => image.content.large.url}
          = image_tag(image.content.medium.url)
    %br

:javascript

  jQuery(function() {
    $("a.fancybox").fancybox({'padding': 0, overlayOpacity: 0.8, overlayColor: '#000000'});
  });
